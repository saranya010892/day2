package day2assignment;



public class PrimeNumber {

	
	public static void main(String[] args) {
	
		int number = 13;
		boolean flag = false;
			for (int i =2;i<= number/2; i++)
			{
		int remainder = number%i;
			if (remainder == 0)
			{
				flag=true;
				break;
			}
		

	}
if(flag ==true)

	
	System.out.println(" prime number");
else
	System.out.println(" not prime number");
	
		
	
}
}
